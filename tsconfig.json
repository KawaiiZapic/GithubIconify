{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "skipLibCheck": true,
        "types": [
            "vite/client"
        ],
        "baseUrl": "."
    },
    "include": [
        "src/**/*.ts"
    ]
}